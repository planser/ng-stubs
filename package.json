{
  "name": "ng-stubs",
  "version": "0.7.2",
  "description": "",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/planser/ng-stubs.git"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "compile": "tsc",
    "test-watch": "jest --watch",
    "test": "jest"
  },
  "jest": {
    "testRegex": "(\\.(test|spec))\\.(ts|js)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/test/setup.js",
    "transform": {
      "^.+\\.(ts)$": "<rootDir>/test/preprocessor.js"
    }
  },
  "types": "dist/index.d.ts",
  "keywords": [
    "Angular",
    "Test",
    "Stub",
    "Component",
    "Directive"
  ],
  "author": "Peter Lanser",
  "license": "MIT",
  "devDependencies": {
    "@angular/common": "^7.0.2",
    "@angular/compiler": "^7.0.2",
    "@angular/core": "^7.0.2",
    "@angular/forms": "^7.0.2",
    "@angular/platform-browser": "^7.0.2",
    "@angular/platform-browser-dynamic": "^7.0.2",
    "@types/jest": "^22.1.1",
    "jest": "^22.1.4",
    "jest-zone-patch": "0.0.8",
    "rxjs": "~6.3.3",
    "ts-jest": "^22.0.3",
    "typescript": "~3.1.6",
    "zone.js": "^0.8.4"
  },
  "peerDependencies": {
    "@angular/common": "^7.0.2",
    "@angular/compiler": "^7.0.2",
    "@angular/core": "^7.0.2",
    "@angular/forms": "^7.0.2",
    "@angular/platform-browser": "^7.0.2",
    "@angular/platform-browser-dynamic": "^7.0.2"
  }
}
